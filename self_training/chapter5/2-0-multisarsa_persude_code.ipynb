   "id": "1e2f032e-16a2-474a-9aeb-51182e683355",
   "metadata": {},
   "source": [
    "init Q(s, a) Table\n",
    "\n",
    "for e in range(1, E):\n",
    "    init s\n",
    "    a = e-greedy(s)\n",
    "\n",
    "    while not done:\n",
    "        r = 0 \n",
    "        for i in range(n_step)\n",
    "            rt, s' = step(s, a)  # !!!\n",
    "            a' = e-greedy(s')    # !!!\n",
    "            r += gamma^(i-1)*rt\n",
    "        q(s, a) += (r + gamma^n *q(s', a') - q(s, a))\n",
    "        s = s'\n",
    "        a = a'"
   ]
  }
